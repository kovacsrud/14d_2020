<Window x:Class="WpfKutyakDb.UjKutyafajta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfKutyakDb"
        mc:Ignorable="d"
        Title="UjKutyafajta" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Kutyafajta neve" FontSize="30" />
            <TextBox x:Name="textboxNev" FontSize="26" />
            <TextBlock Text="Kutyafajta eredeti neve" FontSize="30" />
            <TextBox x:Name="textboxEredetinev" FontSize="26" />
            <Button x:Name="buttonInsertUjfajta" Content="Adat felvitele" Margin="20" Width="200" FontSize="26" />
        </StackPanel>
    </Grid>
</Window>
